    "# Convert an Array into a 2D array with conditions\n",
    "\n",
    "def findMatrix(nums):\n",
    "    l = {}\n",
    "    for i in nums:\n",
    "        l[i] = l.get(i,0) + 1\n",
    "        print(l)\n",
    "\n",
    "        \n",
    "\n",
    "#num = [list(int(input()))]\n",
    "nums = [1,3,4,1,2,3,1]\n",
    "res =findMatrix(nums)\n",
    "print(res)"
   ]
